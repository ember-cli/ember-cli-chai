{
  "name": "ember-cli-chai",
  "version": "0.5.0",
  "description": "Chai assertions for Ember.js",
  "keywords": [
    "chai",
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Jonathan kingston",
  "files": [
    "addon",
    "rollup",
    "vendor",
    "index.js"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-cli/ember-cli-chai.git",
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "start": "ember serve",
    "test": "ember test"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-rollup": "^2.1.1",
    "chai": "^4.1.2",
    "ember-cli-version-checker": "^3.0.0",
    "resolve": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1"
  },
  "devDependencies": {
    "chai-as-promised": "^7.0.0",
    "chai-jquery": "^2.0.0",
    "ember-cli": "~3.8.0",
    "ember-cli-babel": "^7.5.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-shims": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-load-initializers": "^2.0.0",
    "ember-resolver": "^5.1.3",
    "ember-sinon": "~3.1.0",
    "ember-source": "~3.8.0",
    "lerna-changelog": "^0.8.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^3.0.0",
    "testdouble-chai": "^0.5.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-testdouble"
    ]
  },
  "changelog": {
    "repo": "ember-cli/ember-cli-chai",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  }
}
